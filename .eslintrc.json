{
	"env": {
		"node": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended","plugin:@typescript-eslint/eslint-recommended"],
	"parserOptions": {
		"ecmaVersion": 13
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "windows"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"]
	}
}
