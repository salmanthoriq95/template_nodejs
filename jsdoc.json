{
	"source": {
		"include": ["src"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/",
		"template": "./jsdocTemplate/",
		"tutorials": "./jsdocTemplate/tutorials/",
		"readme": "./readme.md"
	}
}
